{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchPackagePatterns": [
        "*"
      ]
    }
  ],
  "labels": ["release-note-none"],
  "extends": [":gitSignOff"],
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "osvVulnerabilityAlerts": true,
  "assigneesFromCodeOwners": true,
  "constraints": {
      "go": "1.20"
  },
  "prConcurrentLimit": 3
}
